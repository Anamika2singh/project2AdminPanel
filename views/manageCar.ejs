    <script type="text/javascript">
        jQuery(document).on('click', '#deletedGlobal', function () {
        var url = jQuery(this).data('href');
        swal({
            title: 'Delete',
            icon: 'error',
            text: 'Are you sure want to delete ? ',
            buttons: { yes: { text: 'Yes', value: 'yes' }, no: { text: 'No', value: 'no' } }
        }).then(function (value) {
            if (value == 'yes') {
                window.location = url;
            }
        });
    });
    </script>
      <script type="text/javascript">
        jQuery(document).on('click', '#deactivate', function () {
        var url = jQuery(this).data('href');
        swal({
            title: 'Are you sure?',
            icon: 'warning',
            text: "You want to Deactivate this Car? ",
            buttons: { yes: { text: 'Yes', value: 'yes' }, no: { text: 'No', value: 'no' } }
        }).then(function (value) {
            if (value == 'yes') {
                window.location = url;
                swal("Car Deactivated Successfully!", {
                 icon: "success",
                 });
            }
        });
       });  
      </script>
       <script type="text/javascript">
        jQuery(document).on('click', '#activate', function () {
        var url = jQuery(this).data('href');
        swal({
            title: "Are you sure?",
            icon: 'warning',
            text: 'Are you sure you want to Activate this Car? ',
            buttons: { yes: { text: 'Yes', value: 'yes' }, no: { text: 'No', value: 'no' } }
        }).then(function (value) {
            if (value == 'yes') {
                window.location = url;
                swal("car Activated Successfully!", {
                 icon: "success",
                 });
            }
        });
       });
