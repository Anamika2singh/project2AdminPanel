                                <!--begin::Body-->
                                <div class="card-body py-0">
                                    <!--begin::Table-->
                                    <div class="table-responsive">
                                        <table class="table table-head-custom table-vertical-center" id="kt_advance_table_widget_1">
                                            <thead>
                                                <tr class="text-left">
                                                    <th style="width: 50px; white-space: nowrap;" class="label-inline">
                                                        Sr. No.
                                                    </th>
                                                    <th>Name</th>
                                                    <th>Image</th>
                                                    <th>Created On</th>
                                                    <th>Updated On</th>
                                                    <th>Action</th>
                                                    <th></th>
                                                </tr>
                                            </thead>


                                            <tbody>
                                                <% let n=1;
                                                data.forEach(function(car){%>
                                                <tr>
                                                    <td>
                                                        <%=n%>
                                                    </td>
                                                    <td>
                                                       <a href="" data-toggle="modal" data-target="#updateSportModal<%=car._id%>"><%=car.name%></a>

                                                         <!-- Modal -->
                                                        <div class="modal fade" id="updateSportModal<%=car._id%>" tabindex="-1" aria-labelledby="updateSportModalLabel" aria-hidden="true">
                                                            <div class="modal-dialog modal-lg">
                                                                <div class="modal-content">
                                                                    <form method="post" action="/car/updateCar/<%=car.image%>/<%=car._id%>" enctype="multipart/form-data">
                                                                    <div class="modal-header">
                                                                        <h5 class="modal-title" id="updateSportModalLabel">Update Car</h5>
                                                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                                <span aria-hidden="true">&times;</span>
                                                              </button>
                                                                    </div>
                                                                    <div class="modal-body">
                                                                        <div class="form-group row">
                                                                            <div class="col-md-12">
                                                                                <label class="font-weight-bold">Car Name</label>
                                                                                <input type="text" name="carName" class="form-control" placeholder="Car Name" value="<%=car.name%>" maxlength="100" required="true">
                                                                            </div>
                                                                        </div>

                                                                      

                                                                        <div class="form-group row">
                                                                            <div class="col-md-12">
                                                                                <label class="font-weight-bold">Insert Image</label>
                                                                                <div class="dropzone dropzone-multi" id="kt_dropzone_5">
                                                                                    <div class="dropzone-panel mb-lg-0 mb-2">
                                                                                       <input type="file" class="dropzone-select  font-weight-bold btn-sm" id="image" name="carImage" accept="image/*">
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                    </div>
                                                                    <div class="modal-footer">
                                                                        <button type="reset" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                                                        <button type="submit" class="btn btn-primary">Update</button>
                                                                    </div>
                                                                    </form>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </td>
                                                    <td>
                                                        <img src="https://velatedocuments.s3.eu-west-2.amazonaws.com/car/<%=car.image%>"  class = "rounded"  width="71px" height="60px">
                                                    </td>
                                                    <td>
                                                        <%=moment(car.createdAt).format('DD/MM/YYYY HH:MM:SS').toString();%>
                                                        
                                                    </td>
                                                    <td>
                                                        <!-- <%=car.updatedAt.toDateString()%> -->
                                                        <%=moment(car.updatedAt).format('DD/MM/YYYY HH:MM:SS').toString();%>
                                                    </td>
                                                    <td>
                                                        <div class="row"></div>
                                                       
															<%if(car.status == 1){%>
																 
																	<div class="col-md-6">
                                                                        <a id= "deactivate" data-href="/car/setAction/<%=car.status%>/<%=car._id%>">
																<span class="svg-icon svg-icon-primary svg-icon-2x"><!--begin::Svg Icon | path:/var/www/preview.keenthemes.com/metronic/releases/2021-05-14-112058/theme/html/demo1/dist/../src/media/svg/icons/General/Settings#3.svg--><svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
																	<g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
																		<rect x="0" y="0" width="24" height="24"/>
																		<rect fill="#000000" opacity="0.3" x="2" y="6" width="21" height="12" rx="6"/>
																		<circle fill="#000000" cx="17" cy="12" r="4"/>
																	</g>
																</svg><!--end::Svg Icon--></span>
															   </div>
                                                            </a>
															  <%}else if(car.status == 0){%>
																
																<div class="col-md-6">
                                                                    <a id= "activate" data-href="/car/setAction/<%=car.status%>/<%=car._id%>">
																	<span class="svg-icon svg-icon-danger svg-icon-2x"><!--begin::Svg Icon | path:/var/www/preview.keenthemes.com/metronic/releases/2021-05-14-112058/theme/html/demo1/dist/../src/media/svg/icons/General/Settings#3.svg--><svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
																		<g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
																			<rect x="0" y="0" width="24" height="24"/>
																			<rect fill="#000000" opacity="0.3" x="2" y="6" width="21" height="12" rx="6"/>
																			<circle fill="#000000" cx="17" cy="12" r="4"/>
																		</g>
																	</svg><!--end::Svg Icon--></span>
                                                                    </a>
																 </div>																 
															   <%}%>   																	 
																 
															</a>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <a id="deletedGlobal" data-href="/car/deleteCar/<%=car._id%>" class="btn btn-icon btn-danger mb-1 btn-sm"> <i class="flaticon-delete"></i> </a>
                                                    </td>
                                                </tr>
                                                <%n++;})%>

                                            </tbody>        
                                            
                                        </table>
                                    </div>
                                    <!--end::Table-->
                                </div>
